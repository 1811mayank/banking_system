<br>
<div class="container content-center">
    <%= form_with url: "withdraw/#{@account.id}", method: :patch do |f| %>
        <div class="form-group">
            <%= f.label :amount ,"Enter amount money"%>
            <%= f.number_field :amount %>
        </div>
        <%= f.submit "Withdraw", class:"btn btn-outline-success my-2 my-sm-0" %>
    <% end %>
    
    <br>
    <%= link_to "atmwithdraw/#{@account.id}", class:"btn btn-outline-success my-2 my-sm-0" do %>
        <%= "ATM Withdrawal"%>
    <%end%> 
   <%= link_to ("Back"), accounts_path, class:"btn btn-outline-success my-2 my-sm-0"%> 
</div>